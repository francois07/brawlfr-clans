{
  "compilerOptions": {
    "jsx": "react",
    "target": "es6",
    "lib": ["esnext", "dom"],
    "module": "commonjs",
    "allowSyntheticDefaultImports": true,
    "strictNullChecks": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "outDir": "dist"
  },
  "include": ["src/**/*.ts"],
  "exclude": ["node_modules"]
}
